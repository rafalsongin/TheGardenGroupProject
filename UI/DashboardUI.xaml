<Window x:Class="TheGardenGroupProject.DashboardUI"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:theGardenGroupProject="clr-namespace:TheGardenGroupProject"
        mc:Ignorable="d"
        Title="Dashboard" Height="450" Width="800">
    <Grid Width="800" Height="450" x:Name="ChartContainer">
        <Button x:Name="ButtonLogout" Content="Logout" HorizontalAlignment="Left" Margin="709,10,0,0" VerticalAlignment="Top" Height="32" Width="81" FontSize="16" AutomationProperties.Name="buttonLogin" Background="#FF282828" Foreground="#FFA3BB88" FontWeight="Bold" Click="buttonLogout_Click" RenderTransformOrigin="-0.228,2.062"/>
        <Label x:Name="LabelDisplayLoggedInUsername" Content="Logged in as: " HorizontalAlignment="Left" HorizontalContentAlignment="Right" Margin="457,10,0,0" VerticalAlignment="Top" FontSize="16" Height="32" Width="247"/>
        
    </Grid>
</Window>

