<Window x:Class="TheGardenGroupProject.MainWindowCompanyEmployee"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:TheGardenGroupProject"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="MainWindowCompanyEmployee" Height="720" Width="1280">
    <Grid>
        <StackPanel MinHeight="700" Width="265" HorizontalAlignment="Left" VerticalAlignment="Center">
            <Button x:Name="DashboardButton" Click="DashboardButton_Click" Content="Dashboard" MinHeight="50" FontSize="20" Margin="10, 20, 10, 0"/>
            <Button x:Name="ViewUsersButton" Click="ViewUsersButton_Click" Content="View Users" MinHeight="50" FontSize="20" Margin="10, 20, 10, 0"/>
            <Button Content="Button" MinHeight="50" FontSize="20" Margin="10, 20, 10, 0"/>
            <Button Content="Button" MinHeight="50" FontSize="20" Margin="10, 20, 10, 0"/>

        </StackPanel>
        <Rectangle HorizontalAlignment="Left" Height="700" Margin="270,10,0,0" VerticalAlignment="Top" Width="2" Fill="Black"/>
        <Frame x:Name="ContentPage" Margin="280,2,0,2" Height="600" Width="900"/>
    </Grid>

</Window>
